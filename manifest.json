{
    "name" : "Get Your PDF",
    "version" : "0.0.1",
    "manifest_version": 3,
    "description" : " Get PDF of current web page.  ",
    "action" : {
        "default_popup": "popup.html",
        "default_icon" : "logo.png"
    },
    "icons" : {
            "128": "logo.png"
    },
    "permissions" : ["activeTab", "scripting", "downloads"],
    "web_accessible_resources": [
  {
    "resources": ["styles.css"],
    "matches": ["<all_urls>"]
  }
]

}